<!DOCTYPE html>
<html lang="en" ng-app="burgerama" data-framework="angularjs">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/bundles/css")
</head>
<body>
    @RenderBody()
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDMSKCEtbHIORL8DuKSxSEXxkBGB-mqf9c&sensor=true&libraries=places"></script>
    <script type="text/javascript" src="https://cdn.auth0.com/w2/auth0-widget-3.0.js"></script>
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/burgerama")
    @RenderSection("scripts", required: false)
    <script type="text/javascript" src="https://cdn.auth0.com/w2/auth0-angular-0.1.js"></script>
</body>
</html>